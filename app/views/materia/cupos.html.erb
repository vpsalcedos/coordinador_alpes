<h1>Cupos de Materias</h1>


<div class="well">
<table>
  <tr>
    <th>id</th>
    <th>Nombre</th>
    <th>Codigo</th>
    <th>Tipo</th>
    <th>Programa</th>
    <th>Creditos</th>
  </tr>
  
  <% @materias.each do |mat| %>

      <tr>
	<td><%= mat.id %></td>
        <td><%= mat.nombre %></td>
        <td><%= mat.codigo %></td>
        <td><%= mat.tipo %></td>
        <td><%= mat.programa %></td>
	<td><%= mat.creditos %></td>
      </tr>

  <% end %>
</table>
</div>
