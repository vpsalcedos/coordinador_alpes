<h1>Cupos de Materias</h1>


<div class="well">
<table>
  <tr>
    <th>id</th>
    <th>Nombre</th>
    <th>Tipo</th>
    <th>Cupos Ultimo Semestre</th>
    <th>Cupos No Ultimo Semestre</th>
  </tr>
  
  <% @materias.each do |mat| %>

      <tr>
	<td><%= mat.id %></td>
        <td><%= mat.nombre %></td>
        <td><%= mat.tipo %></td>
        <td><%= mat.cupoUltimoSemestre %></td>
        <td><%= mat.cupo %></td>
      </tr>

  <% end %>
</table>
</div>
